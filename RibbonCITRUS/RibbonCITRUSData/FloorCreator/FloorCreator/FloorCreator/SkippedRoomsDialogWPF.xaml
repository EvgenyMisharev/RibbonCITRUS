<Window x:Class="FloorCreator.SkippedRoomsDialogWPF"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:FloorCreator"
             mc:Ignorable="d" 
             d:DesignHeight="400" d:DesignWidth="400"
             Height="400" Width="400" MinHeight="400" MinWidth="400"
             WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen" KeyDown="SkippedRoomsDialogWPF_KeyDown" Title="В помещениях не заполнен параметр &quot;Отделка пола&quot;">
    <Grid>
        <ListBox x:Name="listBox_SkippedRooms" Margin="10,10,10,50" MouseDoubleClick ="listBox_SkippedRooms_MouseDoubleClick"/>
        <Button x:Name="btn_Ok" Content="Ок" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="100" Height="25" Click="btn_Ok_Click"/>
    </Grid>
</Window>
